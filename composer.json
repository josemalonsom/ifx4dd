{
    "name": "josemalonsom/ifx4dd",
    "description": "Informix Platform for Doctrine DBAL",
    "keywords": ["informix", "doctrine", "dbal", "database", "persistence", "queryobject"],
    "homepage": "http://www.github.com/josemalonsom/ifx4dd",
    "license": "MIT",
    "authors": [
        {"name": "Jose M. Alonso M.", "email": "josemalonsom@yahoo.es", "role": "monkey hitting keys"}
    ],
    "require": {
        "ext-pdo_informix": ">=1.3.1",
        "doctrine/dbal": "~2.6"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^5.4.6",
        "phpunit/phpunit-mock-objects": "!=3.2.4,!=3.2.5",
        "symfony/console": "2.*||^3.0"
    },
    "autoload": {
        "psr-0": { "Doctrine\\DBAL\\": "lib/" }
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml*", "build.xml"]
    }
}
